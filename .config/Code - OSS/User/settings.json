{
  "keyboard.dispatch": "keyCode",
  "vim.sneak": true,
  "vim.useCtrlKeys": true,
  "editor.fontFamily": "'Inconsolata for Powerline', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.cursorStyle": "block",
  "editor.formatOnSave": false,
  "editor.fontSize": 11,
  "editor.tabSize": 2,
  "editor.renderWhitespace": "boundary",
  "editor.autoClosingBrackets": "never",
  "editor.autoSurround": "never",
  "editor.dragAndDrop": false,
  "editor.copyWithSyntaxHighlighting": false,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "explorer.confirmDelete": false,
  "workbench.sideBar.location": "left",
  "extensions.ignoreRecommendations": true,
  "flow.enabled": true,
  "flow.showStatus": true,
  "flow.runOnEdit": false,
  "flow.useLSP": true,
  "flow.showUncovered": true,
  "flow.useNPMPackagedFlow": true,
  "flow.pathToFlow": "${workspaceFolder}/node_modules/.bin/flow",
  "editor.suggest.localityBonus": true,
  "files.exclude": {
    "**/coverage/**": true,
    "**/dist/**": true,
    "**/flow-coverage/**": true,
    "**/node_modules/**": true
  },
  "files.trimTrailingWhitespace": true,
  "workbench.useExperimentalGridLayout": true,
  "workbench.activityBar.visible": false,
  "breadcrumbs.enabled": false,
  "workbench.editor.closeOnFileDelete": true,
  "search.location": "panel",
  "vim.easymotion": true,
  "vim.foldfix": true,
  "vim.searchHighlightColor": "#666666",
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.insert": "#1e5ed0",
  "vim.statusBarColors.normal": "#de5e1e",
  "vim.statusBarColors.replace": "#de1e1e",
  "vim.statusBarColors.visual": "#aaaa00",
  "vim.statusBarColors.visualblock": "#aeaa00",
  "vim.statusBarColors.visualline": "#aaaa00",
  "vim.substituteGlobalFlag": true,
  "vim.textwidth": 100,
  "vim.incsearch": true,
  "vim.hlsearch": true,
  "vim.useSystemClipboard": true,
  "vim.enableNeovim": true,
  "vim.neovimPath": "/usr/bin/nvim",
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-p>": false,
    "<C-f>": false
  },
  "editor.minimap.enabled": false,
  "window.zoomLevel": 0,
  "workbench.colorCustomizations": {
    "statusBar.background": "#de5e1e",
    "statusBar.noFolderBackground": "#de5e1e",
    "statusBar.debuggingBackground": "#de5e1e"
  },
  "terminal.external.osxExec": "Alacritty.app",
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "npm.packageManager": "yarn",
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.currentLine.enabled": false,
  "gitlens.blame.highlight.enabled": false,
  "gitlens.codeLens.enabled": false,
  "javascript.implicitProjectConfig.checkJs": false,
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "javascript.preferences.quoteStyle": "single",
  "javascript.referencesCodeLens.enabled": true,
  "javascript.suggest.completeFunctionCalls": true,
  "typescript.disableAutomaticTypeAcquisition": false,
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.preferences.quoteStyle": "single",
  "typescript.referencesCodeLens.enabled": true,
  "typescript.suggest.completeFunctionCalls": true,
  "typescript.tsserver.log": "verbose",
  "typescript.tsserver.trace": "messages",
  "tsserver.enable": true,
  "tsserver.formatOnType": true,
  "tsserver.npm": "yarn",
  "tsserver.implicitProjectConfig.checkJs": false,
  "tsserver.implicitProjectConfig.experimentalDecorators": false,
  "tsserver.disableAutomaticTypeAcquisition": false,
  "typescript.suggest.enabled": true,
  "typescript.validate.enable": true,
  "javascript.suggest.enabled": true,
  "javascript.validate.enable": false,
  "javascript.updateImportsOnFileMove.enabled": "always"
}
