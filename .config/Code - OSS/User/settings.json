{
  "keyboard.dispatch": "keyCode",
  "vim.sneak": true,
  "vim.useCtrlKeys": true,
  "editor.fontFamily": "'JetBrains Mono','Inconsolata for Powerline', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.cursorStyle": "block",
  "editor.formatOnSave": true,
  "editor.fontSize": 11,
  "editor.tabSize": 2,
  "editor.renderWhitespace": "boundary",
  "editor.autoClosingBrackets": "never",
  "editor.autoSurround": "never",
  "editor.dragAndDrop": false,
  "editor.copyWithSyntaxHighlighting": false,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "explorer.confirmDelete": false,
  "workbench.sideBar.location": "left",
  "extensions.ignoreRecommendations": true,
  "flow.showStatus": true,
  "flow.runOnEdit": false,
  "flow.useLSP": true,
  "flow.showUncovered": true,
  "flow.useNPMPackagedFlow": true,
  "flow.pathToFlow": "${workspaceFolder}/node_modules/.bin/flow",
  "editor.suggest.localityBonus": true,
  "files.exclude": {
    "**/coverage/**": true,
    "**/dist/**": true,
    "**/flow-coverage/**": true,
    "**/node_modules/**": true
  },
  "files.trimTrailingWhitespace": true,
  "workbench.useExperimentalGridLayout": true,
  "workbench.activityBar.visible": true,
  "breadcrumbs.enabled": true,
  "workbench.editor.closeOnFileDelete": true,
  "vim.easymotion": true,
  "vim.foldfix": true,
  "vim.searchHighlightColor": "#666666",
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.insert": "#1e5ed0",
  "vim.statusBarColors.normal": "#de5e1e",
  "vim.statusBarColors.replace": "#de1e1e",
  "vim.statusBarColors.visual": "#aaaa00",
  "vim.statusBarColors.visualblock": "#aeaa00",
  "vim.statusBarColors.visualline": "#aaaa00",
  "vim.gdefault": true,
  "vim.textwidth": 100,
  "vim.incsearch": true,
  "vim.hlsearch": true,
  "vim.useSystemClipboard": true,
  "vim.enableNeovim": true,
  "vim.neovimPath": "/usr/bin/nvim",
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-p>": false,
    "<C-f>": false
  },
  "editor.minimap.enabled": true,
  "window.zoomLevel": -1,
  "workbench.colorCustomizations": {
    "statusBar.background": "#de5e1e",
    "statusBar.noFolderBackground": "#de5e1e",
    "statusBar.debuggingBackground": "#de5e1e"
  },
  "terminal.external.osxExec": "Alacritty.app",
  "merge-conflict.autoNavigateNextConflict.enabled": true,
  "npm.packageManager": "yarn",
  "gitlens.hovers.currentLine.over": "line",
  "gitlens.currentLine.enabled": false,
  "gitlens.blame.highlight.enabled": false,
  "gitlens.codeLens.enabled": false,
  "javascript.preferences.importModuleSpecifier": "non-relative",
  "javascript.preferences.quoteStyle": "single",
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.preferences.quoteStyle": "single",
  "typescript.tsserver.log": "terse",
  "tsserver.enable": false,
  "tsserver.formatOnType": true,
  "tsserver.npm": "yarn",
  "tsserver.implicitProjectConfig.checkJs": false,
  "tsserver.implicitProjectConfig.experimentalDecorators": false,
  "tsserver.disableAutomaticTypeAcquisition": false,
  "reason_language_server.per_value_codelens": true,
  "reason_language_server.show_debug_errors": true,
  "reason_language_server.useOldDuneProcess": false,
  "vsicons.dontShowNewVersionMessage": true,
  "javascript.autoClosingTags": false,
  "typescript.autoClosingTags": false,
  "typescript.locale": "en",
  "typescript.updateImportsOnFileMove.enabled": "always",
  "php.validate.enable": false,
  "php.suggest.basic": false,
  "emmet.showExpandedAbbreviation": "inMarkupAndStylesheetFilesOnly",
  "flow.logLevel": "error",
  "javascript.format.enable": false,
  "javascript.format.insertSpaceAfterKeywordsInControlFlowStatements": false,
  "javascript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
  "javascript.format.insertSpaceAfterCommaDelimiter": false,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": false,
  "javascript.format.insertSpaceAfterSemicolonInForStatements": false,
  "javascript.format.insertSpaceBeforeAndAfterBinaryOperators": false,
  "javascript.preferences.renameShorthandProperties": false,
  "javascript.suggest.completeJSDocs": false,
  "prettier.trailingComma": "all",
  "prettier.configPath": ".prettierrc",
  "editor.suggest.showSnippets": false,
  "editor.snippetSuggestions": "none",
  "editor.suggest.snippetsPreventQuickSuggestions": false,
  "javascript.updateImportsOnFileMove.enabled": "always",
  "typescript.format.enable": false,
  "typescript.format.insertSpaceAfterCommaDelimiter": false,
  "typescript.format.insertSpaceAfterFunctionKeywordForAnonymousFunctions": false,
  "typescript.format.insertSpaceAfterKeywordsInControlFlowStatements": false,
  "typescript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": false,
  "typescript.format.insertSpaceAfterSemicolonInForStatements": false,
  "typescript.format.insertSpaceBeforeAndAfterBinaryOperators": false,
  "explorer.confirmDragAndDrop": false,
  "typescript.surveys.enabled": false,
  "debug.console.fontSize": 11,
  "markdown.preview.fontSize": 12,
  "terminal.integrated.fontSize": 13,
  "terminal.integrated.fontFamily": "'Inconsolata for Powerline'",
  "vim.easymotionMarkerFontFamily": "'Inconsolata for Powerline'",
  "diffEditor.ignoreTrimWhitespace": true,
  "cSpell.userWords": [
    "Formik",
    "flowlint",
    "flowtype",
    "minmax",
    "noflip",
    "nofolow",
    "noindex",
    "postgres",
    "promisify",
    "sonarjs",
    "unfetch"
  ],
  "flow.useCodeSnippetOnFunctionSuggest": false,
  "typescript.check.npmIsInstalled": false,
  "typescript.preferences.renameShorthandProperties": false,
  "typescript.suggest.completeJSDocs": false,
  "cSpell.enabled": false,
  "javascript.suggest.includeAutomaticOptionalChainCompletions": false,
  "typescript.disableAutomaticTypeAcquisition": true,
  "typescript.reportStyleChecksAsWarnings": false,
  "typescript.suggest.includeAutomaticOptionalChainCompletions": false,
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "typescript.implementationsCodeLens.enabled": true,
  "typescript.referencesCodeLens.enabled": true,
  "editor.wordWrapColumn": 100,
  "javascript.implicitProjectConfig.checkJs": true,
  "javascript.referencesCodeLens.enabled": true,
  "typescript.tsserver.maxTsServerMemory": 6000,
  "gitlens.insiders": true,
  "typescript.workspaceSymbols.scope": "currentProject",
  "typescript.referencesCodeLens.showOnAllFunctions": true,
  "typescript.preferences.useAliasesForRenames": false,
  "typescript.preferences.importModuleSpecifierEnding": "minimal",
  "typescript.enablePromptUseWorkspaceTsdk": true,
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}