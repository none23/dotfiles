{
  "javascript.validate.enable": false,
  "keyboard.dispatch": "keyCode",
  "vim.sneak": true,
  "vim.useCtrlKeys": true,
  "editor.fontFamily": "'Inconsolata for Powerline', 'monospace', monospace, 'Droid Sans Fallback'",
  "editor.cursorStyle": "block",
  "editor.formatOnSave": false,
  "editor.fontSize": 11,
  "editor.tabSize": 2,
  "editor.renderWhitespace": "boundary",
  "editor.autoClosingBrackets": "never",
  "editor.autoSurround": "never",
  "editor.dragAndDrop": false,
  "editor.copyWithSyntaxHighlighting": false,
  "telemetry.enableCrashReporter": false,
  "telemetry.enableTelemetry": false,
  "workbench.iconTheme": "vscode-icons",
  "explorer.confirmDelete": false,
  "workbench.sideBar.location": "left",
  "extensions.ignoreRecommendations": true,
  "flow.enabled": true,
  "flow.showStatus": true,
  "flow.runOnEdit": false,
  "flow.useLSP": true,
  "flow.showUncovered": true,
  "flow.useNPMPackagedFlow": true,
  "flow.pathToFlow": "${workspaceFolder}/node_modules/.bin/flow",
  "editor.suggest.localityBonus": true,
  "files.exclude": {
    "**/coverage/**": true,
    "**/dist/**": true,
    "**/flow-coverage/**": true,
    "**/node_modules/**": true
  },
  "files.trimTrailingWhitespace": true,
  "workbench.useExperimentalGridLayout": true,
  "workbench.activityBar.visible": false,
  "breadcrumbs.enabled": false,
  "workbench.editor.closeOnFileDelete": true,
  "search.location": "panel",
  "vim.easymotion": true,
  "vim.foldfix": true,
  "vim.searchHighlightColor": "#666666",
  "vim.statusBarColorControl": true,
  "vim.statusBarColors.insert": "#1e5ed0",
  "vim.statusBarColors.normal": "#de5e1e",
  "vim.statusBarColors.replace": "#de1e1e",
  "vim.statusBarColors.visual": "#aaaa00",
  "vim.statusBarColors.visualblock": "#aeaa00",
  "vim.statusBarColors.visualline": "#aaaa00",
  "vim.substituteGlobalFlag": true,
  "vim.textwidth": 100,
  "vim.incsearch": true,
  "vim.hlsearch": true,
  "vim.useSystemClipboard": true,
  "vim.enableNeovim": true,
  "vim.neovimPath": "/usr/bin/nvim",
  "vim.leader": "<space>",
  "vim.handleKeys": {
    "<C-p>": false,
    "<C-f>": false
  },
  "editor.minimap.enabled": false,
  "window.zoomLevel": -1,
  "workbench.colorCustomizations": {
    "statusBar.background": "#de5e1e",
    "statusBar.noFolderBackground": "#de5e1e",
    "statusBar.debuggingBackground": "#de5e1e"
  },
  "terminal.external.osxExec": "Alacritty.app",
  "javascript.autoClosingTags": false,
  "javascript.format.insertSpaceAfterConstructor": true,
  "javascript.format.insertSpaceAfterKeywordsInControlFlowStatements": false,
  "javascript.format.insertSpaceAfterOpeningAndBeforeClosingNonemptyBraces": false,
  "javascript.format.insertSpaceBeforeAndAfterBinaryOperators": false,
  "javascript.format.insertSpaceAfterSemicolonInForStatements": false,
  "javascript.preferences.renameShorthandProperties": false,
  "javascript.suggest.autoImports": false,
  "javascript.suggest.completeJSDocs": false,
  "javascript.suggest.enabled": false,
  "javascript.suggest.paths": false,
  "javascript.suggest.names": false,
  "javascript.suggestionActions.enabled": false,
  "javascript.updateImportsOnFileMove.enabled": "never",
  "typescript.autoClosingTags": false,
  "typescript.check.npmIsInstalled": false,
}
