{
  "suggest.timeout": 5000,
  "prettier.semi": true,
  "prettier.trailingComma": "all",
  "prettier.disableSuccessMessage": true,
  "codeLens.enable": false,
  "eslint.autoFix": true,
  "eslint.packageManager": "yarn",
  "eslint.enable": true,
  "eslint.quiet": true,
  "eslint.run": "onSave",
  "eslint.filetypes": [
    "typescript",
    "typescript.tsx",
    "typescriptreact",
    "javascript",
    "javascript.jsx",
    "javascriptreact"
  ],
  "tsserver.enable": true,
  "tsserver.implicitProjectConfig.checkJs": false,
  "tsserver.enableJavascript": true,
  "tsserver.disableAutomaticTypeAcquisition": true,
  "tsserver.formatOnType": false,
  "tsserver.reportStyleChecksAsWarnings": false,
  "javascript.suggest.enabled": true,
  "typescript.suggest.enabled": true,
  "javascript.validate.enable": false,
  "typescript.validate.enable": true,
  "javascript.format.enabled": false,
  "typescript.format.enabled": false,
  "typescript.updateImportsOnFileMove.enable": true,
  "typescript.showUnused": false,
  "typescript.preferences.importModuleSpecifier": "non-relative",
  "typescript.tsserver.experimental.enableProjectDiagnostics": true,
  "languageserver": {
    "tsserver": {
      "enable": false,
      "filetypes": [
        "typescript",
        "typescript.tsx",
        "typescriptreact",
        "javascript",
        "javascript.jsx",
        "javascriptreact"
      ],
      "module": "node"
    },
    "flow": {
      "command": "flow",
      "args": ["lsp"],
      "filetypes": ["javascript", "javascriptreact", "javascript.jsx"],
      "initializationOptions": {},
      "requireRootPattern": true,
      "settings": {},
      "rootPatterns": [".flowconfig"]
    },
    "dockerfile": {
      "command": "docker-langserver",
      "filetypes": ["dockerfile"],
      "args": ["--stdio"]
    },
    "bash": {
      "command": "bash-language-server",
      "args": ["start"],
      "filetypes": ["sh", "zsh", "bash", "shell"]
    },
    "reason": {
      "command": "~/.config/nvim/reason-language-server",
      "filetypes": ["reason"],
      "rootPatterns": ["bsconfig.json"]
    }
  }
}
